 if (_hspeed&lt;&gt;0 and _vspeed&lt;&gt;0) { 
  _hspeed*=0.98;
  x+=_hspeed;
  _vspeed+=arc;
  y+=_vspeed;
  }
  else {
   y-=1;
   }
 
 //Destruction:
 if destroyed=true instance_destroy();
  
end;
