            <string>//Update Camera
with (obj_player)
{
    var _xOff=0, _yOff=0;
    if (instance_exists(screenShakeObj))
    {
        _xOff = screenShakeObj._xOff;
        _yOff = screenShakeObj._yOff;
    }
    
    var __vx, vy;
    __vx=x-view_wview[0]/2;
    __vy=y-view_hview[0]/2; 
    
    //If close, we snap
    if (abs(x - (_camx + view_wview[0]/2)) &lt;= 3)
        _camx = __vx;
    if (abs(y - (_camy + view_hview[0]/2)) &lt;= 3)
        _camy = __vy;
        
    //If not close enough, we move slowly
    if (__vx != _camx)
        _camx += sign(__vx - _camx) * max(abs(_hspeed), 3);
    if (__vy != _camy)
        _camy += sign(__vy - _camy) * max(abs(_vspeed), 3);
        
    view_xview[0] = _camx + (_xOff - cameraObj._lastXOff);
    view_yview[0] = _camy + (_yOff - cameraObj._lastYOff);
    cameraObj._lastXOff=_xOff;
    cameraObj._lastYOff=_yOff;
    
    view_xview[0] = clamp(view_xview[0], 0, room_width - view_wview[0]);
    view_yview[0] = clamp(view_yview[0], 0, room_height - view_hview[0]);
}

instance_deactivate_object(deactivateParObj);
instance_activate_region(view_xview[0]-320,view_yview[0]-320,
                         view_wview[0]+640,view_hview[0]+640,true);
instance_activate_object(obj_player);
</string>
