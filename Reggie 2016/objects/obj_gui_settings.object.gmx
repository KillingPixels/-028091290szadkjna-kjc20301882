            <string>var i,a,xx,yy,list_array,str;

//Set up GUI x and y:
xx=gui_width()/2-sprite_get_width(sprite_index)/2;
yy=gui_height()/2-sprite_get_height(sprite_index)/2;

//Draw own sprite:
draw_sprite(spr_window_settings,0,xx,yy);

//Tabs:
switch (mode) begin

 case 0: //Controls
   if gui_draw_rectangle_clickable(xx+76,yy+35,69,18,true)=true {
    mode=1;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+147,yy+35,69,18,true)=true {
    mode=2;    
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+218,yy+35,69,18,true)=true {
    mode=3;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+289,yy+35,69,18,true)=true {
    mode=4;
    scroll=0;
    }
   draw_sprite(spr_window_settings_tab,0,xx+5,yy+35);
  break;

 case 1: //Gameplay
   if gui_draw_rectangle_clickable(xx+5,yy+35,69,18,true)=true {
    mode=0;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+147,yy+35,69,18,true)=true {
    mode=2;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+218,yy+35,69,18,true)=true {
    mode=3;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+289,yy+35,69,18,true)=true {
    mode=4;
    scroll=0;
    }
    draw_sprite(spr_window_settings_tab,1,xx+76,yy+35);
  break;
  
 case 2: //Audio
   if gui_draw_rectangle_clickable(xx+5,yy+35,69,18,true)=true {
    mode=0;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+76,yy+35,69,18,true)=true {
    mode=1;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+218,yy+35,69,18,true)=true {
    mode=3;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+289,yy+35,69,18,true)=true {
    mode=4;
    scroll=0;
    }
   draw_sprite(spr_window_settings_tab,2,xx+147,yy+35);
  break;
  
 case 3: //Video
   if gui_draw_rectangle_clickable(xx+5,yy+35,69,18,true)=true {
    mode=0;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+76,yy+35,69,18,true)=true {
    mode=1;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+147,yy+35,69,18,true)=true {
    mode=2;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+289,yy+35,69,18,true)=true {
    mode=4;
    scroll=0;
    }
   draw_sprite(spr_window_settings_tab,3,xx+218,yy+35);
  break;
  
 case 4: //Other
   if gui_draw_rectangle_clickable(xx+5,yy+35,69,18,true)=true {
    mode=0;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+76,yy+35,69,18,true)=true {
    mode=1;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+147,yy+35,69,18,true)=true {
    mode=2;
    scroll=0;
    }
   if gui_draw_rectangle_clickable(xx+218,yy+35,69,18,true)=true {
    mode=3;
    scroll=0;
    }
   draw_sprite(spr_window_settings_tab,4,xx+289,yy+35);
  break;
  
 default:
  break;

end;

//Scroll draw:
switch (mode) begin

 case 0: //Controls
  max_rows=max(global.sett_controls,1);
  break;
 
 case 1: //Gameplay
  max_rows=max(global.sett_gameplay,1);
  break;
 
 case 2: //Audio
  max_rows=max(global.sett_sound,1);
  break;
 
 case 3: //Video
  max_rows=max(global.sett_graphics,1);
  break;
 
 case 4: //Other
  max_rows=max(global.sett_misc,1);
  break;
 
 default:
  break;

end;

if gui_draw_sprite_clickable(spr_window_button_scroll,0,xx+344,yy+60,true)
or mouse_wheel_up() {
 if scroll&gt;0 scroll-=1;
 }
 
if gui_draw_sprite_clickable(spr_window_button_scroll,1,xx+344,yy+240,true)
or mouse_wheel_down() {
 if scroll&lt;max_rows-10 scroll+=1;
 }

scroll=gui_draw_scroll_bar(xx+344,yy+80,xx+354,yy+238,max_rows,10,scroll,placeholderSpr);

//List draw:
list_array=sett_list_group(mode,scroll);

draw_set_halign(fa_left);
draw_set_valign(fa_top);
draw_set_font(f_windows);

for (i=0; i&lt;=9; i+=1) begin

 if list_array[i]&lt;&gt;-1 {
  
  if gui_draw_sprite_hover(spr_window_settings_field,0,xx+8,yy+60+i*20)=true {
   str=sett_fetch_desc(list_array[i]);
   draw_set_colour(c_black);
   draw_text_ext(xx+10,yy+267,str,15,345);
   if mode=0 {
    a=keyboard_get_input();
    str='#To change a key binding press and hold the new key then press "Change".#Currently pressed button: '+a[1];
    draw_set_colour(c_dkgray);
    draw_text_ext(xx+10,yy+288,str,13,345);
    }
   }
  
  draw_set_colour(c_black);  
  draw_text(xx+12,yy+60+i*20,global.sett_grid[#0,list_array[i]]);
  
  if mode&lt;&gt;0
   draw_text(xx+142,yy+61+i*20,force_string(sett_get_value(global.sett_grid[#0,list_array[i]])))
   else 
    draw_text(xx+142,yy+61+i*20,key_to_string(sett_get_value(global.sett_grid[#0,list_array[i]])));
    
  sett_draw_widget(global.sett_grid[#0,list_array[i]],global.sett_grid[#1,list_array[i]],list_array[i],xx+211,yy+63+i*20);
  
  }
  
draw_set_alpha(1);
  
end;


//Close button:
if gui_draw_sprite_clickable_rel(spr_window_close,0,xx+246,yy+351)=true
 instance_destroy();
