if (!game_paused()) {

  switch (a) begin

    case 1:
      //Stationary
      break;
    
    case 2:
      //Walking to the right
     image_xscale=-1;
      x+=1.5;
      if (x&gt;=720) {
        a+=1;
        }
      break;
    
    case 3:
      //Waiting
      wait-=1;
      if (wait==0) {
        a+=1;
        instance_create(x+32,y,obj_hospital_doctor);
        }
     break;
    
    case 4:
      //Walking left with a doctor
      image_xscale=1;
      x-=1.5;
      if (x&lt;=500) {
        a+=1;
        }
      break;
      
    case 6:
      //Stand in place
      break;
      
    case 7:
      //Walking to the right again
      image_xscale=-1;
      x+=1.5;
      break;
      
    case 8:
      //Stand in place
      break;
      
    case 9:
      if (x&gt;560) {
        image_xscale=1;
        x-=1.5;
        }
      else {
        _dialogue=2;
        a+=1;
        }
      break;
     
    default:
      break;

    end;
    
  }
</string>
